
@model IEnumerable<DimensionWebApp.Models.Cmpg323Project2Dataset3>
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Employee.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <h3>Employees</h3>
    </div>

</div>
<div class="row">
    <div class="col-md-12">
        <a href="/Cmpg323Project2Dataset3/Create" class="btn btn-primary">Add New Employee</a>
    </div>

</div>

<div class="row">
    <div class="col-md-12">
        <table class="table table-striped">
            <tr>
                <th>EmpNumber</th>
                <th>EmpAge</th>
                <th>EmpGender</th>
                <th>EmpMaritalStatus</th>
                <th>EmpDepartment</th>
                <th>EmpJobRole</th>
                <th>EmpEducationField</th>
                <th>Action</th>
               
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.EmployeeNumber</td>
                    <td>@item.Age</td>
                    <td>@item.Gender</td>
                    <td>@item.MaritalStatus</td>
                    <td>@item.Department</td>
                    <td>@item.JobRole</td>
                    <td>@item.EducationField</td>

                    <td>
                        <a href="#" class="btn" btn-success>Edit</a>
                        <a href="#" class="btn" btn-success>Delete</a>
                    </td>
                </tr>
            }

        </table>

    </div>

</div>

